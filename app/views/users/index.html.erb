<div class="container ctn-2">
  <table class="table">
  	<thead class="thead thead-default">
  		<tr>
  			<th>Name</th>
  			<th>Email</th>
  			<th>Created At</th>
  		</tr>
  	</thead>
  	<tbody>
  		<% @users.each do |user| %>
  				<tr>
  					<td><%= user.name%></td>
  					<td><%= user.email %></td>
  					<td><%= time_ago_in_words user.created_at%></td>
  				</tr>
  		<% end %>
  	</tbody>
  </table>
</div>
